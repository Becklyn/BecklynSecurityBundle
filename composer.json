{
    "name": "becklyn/security-bundle",
    "description": "Security related helpers for symfony apps.",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/SecurityBundle.git",
    "require": {
        "php": ">=7.4",
        "symfony/config": "^4.4 || ^v5.4.3 || ^v6.0.3",
        "symfony/dependency-injection": "^4.4 || ^v5.4.3 || ^v6.0.3",
        "symfony/http-kernel": "^4.4 || ^v5.4.3 || ^v6.0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^4.4 || ^v5.4.3 || ^v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\SecurityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\SecurityBundle\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
